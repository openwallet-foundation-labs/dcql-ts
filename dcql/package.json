{
  "name": "dcql",
  "version": "2.0.1",
  "description": "Digital Credentials Query Language (DCQL)",
  "license": "Apache-2.0",
  "author": "Martin Auer",
  "private": false,
  "publishConfig": {
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.mts",
    "exports": {
      ".": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.mjs",
        "types": "./dist/index.d.mts"
      },
      "./package.json": "./package.json"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown src/index.ts --format esm --dts --clean --sourcemap",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "valibot": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:"
